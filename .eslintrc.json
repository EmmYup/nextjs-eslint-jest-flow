{
  "env": {
    "browser": true
  },
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "airbnb", "prettier", "prettier/react"],
  "rules": {
    "prettier/prettier": [
      "error",
      { "singleQuote": true, "trailingComma": "all" }
    ],
    "react/jsx-filename-extension": [
      "error",
      { "extensions": [".js", ".jsx"] }
    ],
    "jsx-a11y/label-has-for": "off"
  },
  "parser": "babel-eslint"
}
